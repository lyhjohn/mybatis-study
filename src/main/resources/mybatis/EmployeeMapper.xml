<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yohan.mybatisstudy.mapper.EmployeeMapper">

  <insert id="insertEmployeeMapper" useGeneratedKeys="true" keyProperty="id">
    insert into employee (id, user_id, employee_name, employee_address)
    values (#{id}, #{user_id}, #{name}, #{address})
  </insert>

</mapper>